{
  "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
          "es2015",
          "dom"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "suppressImplicitAnyIndexErrors": true,
      "target": "es5",
      "baseUrl": "src",
      "paths": {
          "@/*": [
              "app/*"
          ]
      }
  }
}
